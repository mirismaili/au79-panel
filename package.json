{
  "name": "au79-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "version:tag": "node -e \"child_process.execSync('git tag v' + require('./package.json').version)\"",
    "sort-dependencies": "node -e \"if(process.env.CI)process.exit(0);P=fs.readFileSync('package.json','utf8');({dependencies:d,devDependencies:D}=p=JSON.parse(P));[k,K]=[d??{},D??{}].map(Object.keys);[s,S]=[k,K].map(x=>[...x].sort());if(s.every((x,i)=>x===k[i])&&S.every((x,i)=>x===K[i]))process.exit();p.dependencies=d&&Object.fromEntries(s.map(k=>[k,d[k]]));p.devDependencies=D&&Object.fromEntries(S.map(K=>[K,D[K]]));fs.writeFileSync('package.json',JSON.stringify(p,null,P.match(/(?<=^\\{\\n?)[ \\t]+/)[0])+'\\n');console.log('\\x1b[32mDependencies have been sorted.\\x1b[0mðŸ‘')\"",
    "postinstall": "npm run sort-dependencies"
  },
  "dependencies": {
    "next": "14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.2",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.6.2"
  },
  "engines": {
    "node": ">=22"
  }
}
